    "aws-sdk": "~2.1082.0",
    "axios": "~0.26.1",
    "babel-node": "~0.0.1-security",
    "body-parser": "1.19.0",
    "cfenv": "1.2.4",
    "crypto-random-string": "3.3.0",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "firebase-admin": "^12.0.0",
    "fluent-ffmpeg": "2.1.2",
    "fs": "0.0.1-security",
    "fs-extra": "9.1.0",
    "gm": "1.23.1",
    "handlebars": "~4.7.7",
    "hapi-auth-bearer-token": "8.0.0",
    "hapi-swagger": "~14.2.5",
    "ical-generator": "~3.0.0",
    "joi": "~17.4.0",
    "jsonwebtoken": "8.5.1",
    "jwt-simple": "~0.5.6",
    "knox": "~0.9.2",
    "lodash": "~4.17.21",
    "log4js": "~6.4.1",
    "md5": "2.3.0",
    "moment": "~2.29.1",
    "moment-range": "~4.0.2",
